<tiles-definitions>
	
	<definition name="attributetypegroup/init" extends="defaultHeaderAndFooterLayout">
		<put-attribute name="title" value="global.attributegroups" />
  		<put-attribute name="form_id" value="attributeTypeGroup" />
  		<put-attribute name="form_action" value="init.do" />
  		<put-attribute name="form_model" value="memBean" />
  		<put-attribute name="form_method" value="post" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/AttributeTypeGroup/Overview.jsp" />
	      <add-attribute value="/jsp/common/LastModificationInfo.jsp" />
	    </put-list-attribute>
	</definition>

	<definition name="attributetypegroup/edit" extends="defaultHeaderAndFooterLayout">
		<put-attribute name="title" value="global.attributegroups" />
		<put-attribute name="title_style" value="global.edit" />
  		<put-attribute name="form_id" value="attributeGroups_edit" />
  		<put-attribute name="form_model" value="memBean" />
