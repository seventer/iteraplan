<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
	"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
	
<tiles-definitions>
	<definition name="successorreport/init" extends="standardLayout">
  		<put-attribute name="navigation" value="/jsp/Navigation.jsp" />
		<put-attribute name="menu" value="/jsp/Menu.jsp" />
		<put-attribute name="breadcrumb" value="/jsp/MainBreadcrumb.jsp" />
		<put-attribute name="header" value="/jsp/Header.jsp" />
		<put-attribute name="title" value="global.successorreport" />
  		<put-attribute name="footer" value="/jsp/Footer.jsp" />
  		<put-attribute name="form_id" value="successorReport_show" />
  		<put-attribute name="form_model" value="dialogMemory" />
  		<put-attribute name="form_method" value="post" />
   		<put-attribute name="form_action" value="requestReport.do" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/SuccessorReport/Manage.jsp" />
  		  <add-attribute value="/jsp/SuccessorReport/Result.jsp" />
	    </put-list-attribute>
	</definition>

	<definition name="successorreport/requestReport" extends="standardLayout">
  		<put-attribute name="navigation" value="/jsp/Navigation.jsp" />
		<put-attribute name="menu" value="/jsp/Menu.jsp" />
		<put-attribute name="breadcrumb" value="/jsp/MainBreadcrumb.jsp" />
		<put-attribute name="header" value="/jsp/Header.jsp" />
		<put-attribute name="title" value="global.successorreport" />
  		<put-attribute name="footer" value="/jsp/Footer.jsp" />
  		<put-attribute name="form_id" value="successorReport_requestReport" />
  		<put-attribute name="form_model" value="dialogMemory" />
  		<put-attribute name="form_method" value="post" />
   		<put-attribute name="form_action" value="requestReport.do" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/SuccessorReport/Manage.jsp" />
  		  <add-attribute value="/jsp/SuccessorReport/Result.jsp" />
	    </put-list-attribute>
	</definition>
	
</tiles-definitions>
