	"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
	
<tiles-definitions>
	
	<definition name="supportingquery/init" extends="defaultHeaderAndFooterLayout">
		<put-attribute name="title" value="global.permission_queries" />
  		<put-attribute name="form_id" value="supportingQuery_show" />
  		<put-attribute name="form_model" value="dialogMemory" />
  		<put-attribute name="form_method" value="post" />
  		<put-attribute name="form_action" value="requestSingleReport.do" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/SupportingQuery/Manage.jsp" />
  		  <add-attribute value="/jsp/SupportingQuery/Results.jsp" />
	    </put-list-attribute>
	</definition>
	
	<definition name="supportingquery/requestSingleReport" extends="defaultHeaderAndFooterLayout">
		<put-attribute name="title" value="global.permission_queries" />
  		<put-attribute name="form_id" value="supportingQuery_requestSingleReport" />
  		<put-attribute name="form_action" value="requestSingleReport.do" />
  		<put-attribute name="form_model" value="dialogMemory" />
  		<put-attribute name="form_method" value="post" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/SupportingQuery/Manage.jsp" />
  		  <add-attribute value="/jsp/SupportingQuery/Results.jsp" />
	    </put-list-attribute>
	</definition>
	
</tiles-definitions>
