<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
	"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
	
<tiles-definitions>
	<definition name="customdashboards/init" extends="defaultHeaderAndFooterLayout">
		<put-attribute name="title" value="global.customDashboardInstances" />
  		<put-attribute name="form_id" value="customDashboardInstances" />
  		<put-attribute name="form_model" value="customDashboardDialogMemory" />
  		<put-attribute name="form_method" value="post" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/CustomDashboard/Manage.jsp" />
	    </put-list-attribute>
	</definition>
	<definition name="customdashboardinstance/show" extends="defaultHeaderAndFooterLayout">
		<put-attribute name="title" value="customDashboard.title" />
  		<put-attribute name="form_id" value="customDashboardInstance" />
  		<put-attribute name="form_model" value="customDashboardDialogMemory" />
  		<put-attribute name="form_method" value="get" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/CustomDashboard/DashboardInstance.jsp" />
	    </put-list-attribute>
	</definition>
</tiles-definitions>
