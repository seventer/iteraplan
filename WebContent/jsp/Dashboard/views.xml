<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
	"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
	
<tiles-definitions>
	<definition name="dashboard/init" extends="defaultHeaderAndFooterLayout">
		<put-attribute name="title" value="manageDashboard.title" />
  		<put-attribute name="form_id" value="dashboard_show" />
  		<put-attribute name="form_model" value="dialogMemory" />
  		<put-attribute name="form_method" value="get" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/Dashboard/Manage.jsp" />
	    </put-list-attribute>
	</definition>
	
	<definition name="dashboard/interchange" extends="defaultHeaderAndFooterLayout">
		<put-attribute name="title" value="manageDashboard.title" />
  		<put-attribute name="form_id" value="dashboard_show" />
  		<put-attribute name="form_model" value="dialogMemory" />
  		<put-attribute name="form_method" value="post" />
  		<put-list-attribute name="contents" >
  		  <add-attribute value="/jsp/Dashboard/Manage.jsp" />
	    </put-list-attribute>
	</definition>
</tiles-definitions>
